@model YummyProject.Models.PhotoGallery
@{
    ViewBag.Title = "CreateGallery";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Fotoğraf Ekle</h3>

<form method="post">

    <label>Foto Url</label>
    @Html.TextAreaFor(a => a.ImageUrl, new { @class = "form-control" })
    <br />

    <button class="btn btn-primary">Kaydet</button>

</form>

